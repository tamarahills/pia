
config pia-config-section
	option password 'password'
	option user 'user'
	option enabled '1'
	option client '1'
	option dev 'tun'
	option proto 'udp'
	option resolv_retry 'infinite'
	option nobind '1'
	option persist_key '1'
	option persist_tun '1'
	option ca '/etc/openvpn/ca.rsa.2048.crt'
	option tls_client '1'
	option remote_cert_tls 'server'
	option auth_user_pass '/etc/openvpn/pialogin.pem'
	option auth_nocache '1'
	option comp_lzo 'yes'
	option verb '1'
	option reneg_sec '0'
	option crl_verify '/etc/openvpn/crl.rsa.2048.pem'
	option keepalive '10 120'
	option float '1'
	option auth 'sha1'
	option cipher 'aes-128-cbc'
	option log '/tmp/openvpn.log'
	option boot '1'
	option remote 'us-east.privateinternetaccess.com'


